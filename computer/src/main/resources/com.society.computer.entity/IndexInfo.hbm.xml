<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.society.computer.entity.IndexInfo" table="index_info">
        <id name="id" type="java.lang.Long">
            <column name="index_id"/>
            <generator class="identity"/>
        </id>
        <property name="title" column="index_title"/>
        <property name="content" column="index_content"/>
    </class>
</hibernate-mapping>