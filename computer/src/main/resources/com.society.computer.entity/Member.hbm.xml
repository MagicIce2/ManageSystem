<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.society.computer.entity.Member" table="stuinfo">
        <id name="id" type="java.lang.Long">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="stuNo" column="stuNo"/>
        <property name="stuDepart" column="stuDepart"/>
        <property name="stuClass" column="stuClass"/>
        <property name="stuName" column="stuName"/>
        <property name="stuPhone" column="stuPhone"/>
        <property name="stuSex" column="stuSex"/>
        <property name="stuQQ" column="stuQQ"/>
        <property name="stuPassword" column="stuPassword"/>
        <property name="stuBirthday" column="stuBirthday"/>
        <property name="stuMe" column="stuMe"/>
        <property name="money" column="money"/>
        <property name="author" column="author"/>
        <property name="del" column="del"/>
    </class>
</hibernate-mapping>