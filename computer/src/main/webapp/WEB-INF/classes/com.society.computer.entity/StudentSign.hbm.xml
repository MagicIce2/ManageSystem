<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.society.computer.entity.StudentSign" table="signinfo">
        <id name="id" type="java.lang.Long">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="stuDepart" column="stuDepart"/>
        <property name="stuClass" column="stuClass"/>
        <property name="stuName" column="stuName"/>
        <property name="del" column="del"/>
    </class>
</hibernate-mapping>